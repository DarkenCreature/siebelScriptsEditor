{
	"name": "siebelscripteditor",
	"displayName": "siebelScriptEditor",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.62.0"
	},
	"categories": [
		"Other",
		"Snippets"
	],
	"activationEvents": [
		"onCommand:siebelscripteditor.helloWorld",
		"onCommand:siebelscripteditor.openFile",
		"onView:wsrepo"
	],
	"main": "./extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
				  "id": "siebelScriptEditor",
				  "title": "Siebel Script Editor",
				  "icon": "src/icon.svg"
				}
			  ]
		},
		"views": {
			"siebelScriptEditor": [
			{	"type": "webview",
				"id": "wsrepo",
				"name": "Workspace and repository version"
			  },
			  {
				"id": "businessServices",
				"name": "Business Services"
			  },
			  {
				"id": "businessServiceServerScripts",
				"name": "Business Service Server Scripts"
			  }
			]
		  },
		"commands": [
			{
				"command": "siebelScripteditor.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "siebelscripteditor.openFile",
				"title": "Open File"
			}
		],
		"snippets": [
			{
				"language": "javascript",
				"path": "./snippets.json"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@types/vscode": "^1.62.0",
		"@vscode/test-electron": "^1.6.2",
		"eslint": "^8.1.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.3",
		"oracledb": "^5.3.0",
		"typescript": "^4.4.4"
	}
}

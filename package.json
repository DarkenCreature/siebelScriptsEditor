{
	"name": "siebelscripteditor",
	"displayName": "Siebel Script Editor",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.62.0"
	},
	"categories": [
		"Other",
		"Snippets"
	],
	"activationEvents": [
		"onCommand:siebelscripteditor.pullScript",
		"onCommand:siebelscripteditor.pushScript",
		"onView:extensionView"
	],
	"main": "./extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
				  "id": "siebelScriptEditor",
				  "title": "Siebel Script Editor",
				  "icon": "./media/icon.svg"
				}
			  ]
		},
		"views": {
			"siebelScriptEditor": [
				{
				"type": "webview",
				"id": "extensionView",
				"name": "Select datasource"
			  },
			  {
				"id": "businessServices",
				"name": "Business Services"
			  },
			  {
				"id": "businessComponents",
				"name": "Business Components"
			  },
			  {
				"id": "applets",
				"name": "Applets"
			  },
			  {
				"id": "applications",
				"name": "Applications"
			  }
			]
		  },
		"commands": [
			{
				"command": "siebelscripteditor.pullScript",
				"title": "Get script from Siebel database",
				"icon": {
					"light": "./media/arrowdown.svg",
					"dark": "./media/arrowdown.svg"
				}
			},
			{
				"command": "siebelscripteditor.pushScript",
				"title": "Push script to Siebel database",
				"icon": {
					"light": "./media/arrowup.svg",
					"dark": "./media/arrowup.svg"
				}
			}
		],
		"menus": {
			"editor/title": [
				{
				"command": "siebelscripteditor.pullScript",
				"group": "navigation"
				},
				{
				"command": "siebelscripteditor.pushScript",
				"group": "navigation"
				}
			]
		},
		"snippets": [
			{
				"language": "javascript",
				"path": "./snippets.json"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@types/vscode": "^1.62.0",
		"@vscode/test-electron": "^1.6.2",
		"eslint": "^8.1.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.3",
		"oracledb": "^5.3.0",
		"typescript": "^4.4.4"
	}
}
